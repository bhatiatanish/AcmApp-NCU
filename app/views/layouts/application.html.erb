<!DOCTYPE html>
<html>
  <head>
    <title>ACMAPP</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
<!-- Start of navbar -->
  	<%= render 'shared/app_navbar' %>
<!-- End of navbar -->
    <div class="container">
      <br>
    <% if flash[:notice] %>
      <div class="alert alert-secondary notice" role="alert">
        <%= flash[:notice] %>
    </div>
    <% end %>
    </div>

    <%= yield %>

<!-- Start of footer -->
   <%=  render 'shared/app_footer' %>
<!-- End of footer -->

  </body>
</html>
